.img-box {
  width: 310rpx;
  height: 310rpx;
  border-radius: 50% 50%;
  animation: go 10s linear infinite;
}

.img-box image {
  width: 100%;
  height: 100%;
  border-radius: 50% 50%;
  border: 1rpx solid gray;
  
}

.img-container {
  width: 750rpx;
  height: 650rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

@keyframes go {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(360deg);
  }
}

.rotateFlag {
  animation-play-state: paused;
}
.rotateFlag1{
  animation-play-state: running;
}

.play-box{

  height: 300rpx;
  width: 750rpx;
  border: 1px solid red;

}
.content-box{
  height: 250rpx;
